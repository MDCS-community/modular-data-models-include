    <xsd:include schemaLocation="physical-quantity-numerator_-denominator_.xsd"/>
    <xsd:complexType name="sensor-type">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="name" type="xsd:string"/>
            <xsd:element minOccurs="0" name="type" type="xsd:string"/>
            <xsd:element minOccurs="0" name="manufacturer" type="xsd:string"/>
            <xsd:element minOccurs="0" name="model" type="xsd:string"/>
            <xsd:element minOccurs="0" name="serial" type="xsd:string"/>
            <xsd:element minOccurs="0" name="calibration-date" type="xsd:dateTime"/>
            <xsd:element minOccurs="0" name="range" type="physical-quantity---type"/>
            <xsd:element minOccurs="0" name="accuracy" type="physical-quantity---type"/>
