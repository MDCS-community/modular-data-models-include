<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified"
    elementFormDefault="unqualified">
    <xsd:include schemaLocation="physical-quantity-1-.xsd"/>
    <xsd:include schemaLocation="physical-quantity--L.xsd"/>
    <xsd:include schemaLocation="physical-quantity--.xsd"/>
    <xsd:include schemaLocation="remote-file.xsd"/>
    <xsd:include schemaLocation="xray-source.xsd"/>
    <xsd:complexType name="xray-diffraction-type">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="instrument-ID" type="xsd:string"/>
            <xsd:element minOccurs="0" name="sample-ID" type="xsd:string"/>
            <xsd:element minOccurs="0" name="user-ID" type="xsd:string"/>
            <xsd:element minOccurs="0" name="metadata" type="xsd:anyType"/>
            <xsd:element minOccurs="0" name="xray-source" type="xray-source-type"> </xsd:element>
            <xsd:element minOccurs="0" name="pattern-data">
                <xsd:annotation>
                    <xsd:appinfo><mdcs>blob</mdcs></xsd:appinfo>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="angle-2-theta"
                            type="physical-quantity-1--type"/>
                        <xsd:element minOccurs="0" name="scattering-vector"
                            type="physical-quantity--L-type"/>
                        <xsd:element name="intensity" type="physical-quantity---type"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="supporting-files" type="remote-file-type"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
