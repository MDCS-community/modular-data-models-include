                                                  <xsd:choice>
                                                  <xsd:element
                                                  name="series-1xxx-wrought-pure-aluminum"/>
                                                  <xsd:element name="series-2xxx-aluminum-copper"/>
                                                  <xsd:element name="series-3xxx-aluminum-manganese"/>
                                                  <xsd:element name="series-4xxx-aluminum-silicon"/>
                                                  <xsd:element name="series-5xxx-aluminum-magnesium"/>
                                                  <xsd:element
                                                  name="series-6xxx-aluminum-magnesium-silicon"/>
                                                  <xsd:element name="series-7xxx-aluminum-zinc"/>
                                                  <xsd:element
                                                  name="series-8xxx-aluminum-plus-other-elements-and-clad-aluminum"/>
                                                  <xsd:element name="unclassified-wrought-aluminum"
                                                  />
                                                  </xsd:choice>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  </xsd:choice>
                                                </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="cobalt">
                                                <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element
                                                  name="cobalt-chromium-nickel-tungsten"/>
                                                  <xsd:element name="cobalt-chromium-tungsten"/>
                                                  <xsd:element name="unclassified-cobalt"/>
                                                  </xsd:choice>
                                                </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="copper">
                                                <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element name="cast-copper">
                                                  <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element name="brass"/>
                                                  <xsd:element name="bronze"/>
                                                  <xsd:element name="copper-nickel"/>
                                                  <xsd:element name="copper"/>
                                                  <xsd:element name="high-copper-alloy"/>
                                                  <xsd:element name="leaded-copper"/>
                                                  <xsd:element name="nickel-silver"/>
                                                  <xsd:element name="unclassified-cast-copper"/>
                                                  </xsd:choice>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  <xsd:element name="wrought-Copper">
                                                  <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element name="beryllium-copper-alloy"/>
                                                  <xsd:element name="brass"/>
                                                  <xsd:element name="bronze"/>
                                                  <xsd:element name="copper-nickel"/>
                                                  <xsd:element name="copper"/>
                                                  <xsd:element name="high-copper-alloy"/>
                                                  <xsd:element name="nickel-silver"/>
                                                  <xsd:element name="unclassified-wrought-copper"/>
                                                  </xsd:choice>
                                                  </xsd:complexType>
                                                  </xsd:element>
                                                  </xsd:choice>
                                                </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="gold"/>
                                            <xsd:element name="lead">
                                                <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element
                                                  name="commercially-pure-or-low-alloyed-lead"/>
                                                  <xsd:element name="lead-antimony"/>
                                                  <xsd:element name="lead-tin"/>
                                                  <xsd:element name="unclassified-lead"/>
                                                  </xsd:choice>
                                                </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="magnesium">
                                                <xsd:complexType>
                                                  <xsd:choice>
                                                  <xsd:element name="cast-magnesium-aluminum"/>
                                                  <xsd:element
                                                  name="cast-magnesium-aluminum-manganese"/>
                                                  <xsd:element name="cast-magnesium-aluminum-zinc"/>
                                                  <xsd:element name="cast-magnesium-rare-earth"/>
                                                  <xsd:element name="cast-magnesium-zinc"/>
                                                  <xsd:element name="commercially-pure-magnesium"/>
                                                  <xsd:element name="unclassified-magnesium"/>
                                                  <xsd:element
                                                  name="wrought-magnesium-aluminum-zinc"/>
                                                  <xsd:element name="wrought-magnesium-lithium"/>
                                                  <xsd:element name="wrought-magnesium-zinc"/>
                                                  </xsd:choice>
                                                </xsd:complexType>
                                            </xsd:element>
                                            <xsd:element name="manganese"> </xsd:element>
                                            <xsd:element name="nickel"/>
                                            <xsd:element name="palladium"> </xsd:element>
                                            <xsd:element name="refractory"/>
                                            <xsd:element name="silicon"> </xsd:element>
                                            <xsd:element name="silver"> </xsd:element>
                                            <xsd:element name="tin"/>
                                            <xsd:element name="titanium"/>
                                            <xsd:element name="unclassified"/>
                                            <xsd:element name="zinc"/>
                                            <xsd:element name="zirconium"/>
                                        </xsd:choice>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:choice>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
</xsd:schema>
